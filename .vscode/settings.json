{
    "rust-analyzer.cargo.features": ["ssr"],

    "rust-analyzer.linkedProjects": [
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml"
    ],
    "rust-analyzer.showUnlinkedFileNotification": false,
    
    "emmet.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
      },
      "tailwindCSS.includeLanguages": {
          "rust": "html",
          "*.rs": "html"
      },
      "files.associations": {
          "*.rs": "rust"
      },
      "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": true
      },
      "css.validate": false,
}